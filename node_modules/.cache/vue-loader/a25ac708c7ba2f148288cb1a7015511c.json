rol\">\r\n      <label for=\"message\">Message</label>\r\n      <textarea\r\n        id=\"message\"\r\n        cols=\"30\"\r\n        rows=\"5\"\r\n        v-model.trim=\"message\"\r\n      ></textarea>\r\n    </div>\r\n    <p class=\"errors\" v-if=\"!formIsValid\">\r\n      Please enter a valid email and non-empty message.\r\n    </p>\r\n    <div class=\"actions\">\r\n      <base-button>Send Message</base-button>\r\n    </div>\r\n  </form>\r\n</template>\r\n\r\n<script>\r\nexport default {\r\n  data() {\r\n    return {\r\n      email: '',\r\n      message: '',\r\n      formIsValid: true,\r\n    };\r\n  },\r\n  methods: {\r\n    submitForm() {\r\n      this.formIsValid = true;\r\n      if (\r\n        this.email === '' ||\r\n        !this.email.includes('@') ||\r\n        this.message === ''\r\n      ) {\r\n        console.log('form is invalid');\r\n        this.formIsValid = false;\r\n        return;\r\n      }\r\n\r\n      this.$store.dispatch('requests/contactCoach', {\r\n        email: this.email,\r\n        message: this.message,\r\n        coachId: this.$route.params.id,\r\n      });\r\n      this.$router.replace('/coaches');\r\n    },\r\n  },\r\n};\r\n</script>\r\n\r\n<style scoped>\r\nform {\r\n  margin: 1rem;\r\n  border: 1px solid #ccc;\r\n  border-radius: 12px;\r\n  padding: 1rem;\r\n}\r\n\r\n.form-control {\r\n  margin: 0.5rem 0;\r\n}\r\n\r\nlabel {\r\n  font-weight: bold;\r\n  margin-bottom: 0.5rem;\r\n  display: block;\r\n}\r\n\r\ninput,\r\ntextarea {\r\n  display: block;\r\n  width: 100%;\r\n  font: inherit;\r\n  border: 1px solid #ccc;\r\n  padding: 0.15rem;\r\n}\r\n\r\ninput:focus,\r\ntextarea:focus {\r\n  border-color: #3d008d;\r\n  background-color: #faf6ff;\r\n  outline: none;\r\n}\r\n\r\n.errors {\r\n  font-weight: bold;\r\n  color: red;\r\n}\r\n\r\n.actions {\r\n  text-align: center;\r\n}\r\n</style>"]}]}